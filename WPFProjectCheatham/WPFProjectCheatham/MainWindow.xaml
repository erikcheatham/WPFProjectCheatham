<Window x:Class="WPFProjectCheatham.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFProjectCheatham"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid Name="Grid1">
        <Label Name="Lbl1" Content="Enter A Number Between 1 - 5" HorizontalAlignment="Left" Margin="25,53,0,0" VerticalAlignment="Top" Width="180"/>
        <TextBox Name="TBox1" HorizontalAlignment="Left" Height="23" Margin="230,56,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextChanged="TextBox_TextChanged"/>
        <Button Name="Btn1" Content="Search" HorizontalAlignment="Left" Margin="230,102,0,0" VerticalAlignment="Top" Width="120" Click="Button_Click"/>

    </Grid>
</Window>
